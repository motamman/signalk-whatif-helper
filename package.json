{
  "name": "signalk-whatif-helper",
  "version": "0.1.0",
  "description": "SignalK webapp for browsing, modifying, and creating paths for testing and simulation",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc -w",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "signalk",
    "signalk-node-server-plugin",
    "signalk-webapp",
    "testing",
    "simulation"
  ],
  "author": "",
