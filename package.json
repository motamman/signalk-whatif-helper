    "appIcon": "whatif.png"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@signalk/server-api": "^2.0.0",
    "@types/express": "^5.0.6",
    "@types/node": "^20.0.0",
    "@types/ws": "^8.5.10",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "@signalk/server-api": "^2.0.0"
  },
  "dependencies": {
    "ws": "^8.16.0"
  }
}
