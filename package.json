{
  "name": "signalk-whatif-helper",
  "version": "0.1.0",
  "description": "SignalK webapp for browsing, modifying, and creating paths for testing and simulation",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsc -w",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write 'src/**/*.ts'",
    "format:check": "prettier --check 'src/**/*.ts'",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "signalk",
    "signalk-node-server-plugin",
    "signalk-webapp",
    "testing",
    "simulation"
  ],
  "author": "Maurice Tamman <maurice@zennora.sv>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/motamman/signalk-whatif-helper.git"
  },
  "bugs": {
    "url": "https://github.com/motamman/signalk-whatif-helper/issues"
  },
  "homepage": "https://github.com/motamman/signalk-whatif-helper#readme",
  "signalk-plugin-enabled-by-default": false,
  "signalk": {
    "appSupport": "webapp",
    "displayName": "What-If Helper",
    "appIcon": "assets/icon_set/icon_128x128.png"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@signalk/server-api": "^2.0.0",
    "@types/express": "^5.0.6",
    "@types/node": "^20.0.0",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "@signalk/server-api": "^2.0.0"
  },
  "dependencies": {
    "ws": "^8.16.0"
  }
}
